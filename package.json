{
  "name": "membrane-channels",
  "version": "1.0.0-dev.0",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/membrane-channels.git"
  },
  "devDependencies": {
    "grunt": "~1.5.3",
    "esbuild": "^0.24.2"
  },
  "scripts": {
    "serve-dev": "npx esbuild ../chipper/dist/js/membrane-channels/js/membrane-channels-main.js --bundle --format=iife --global-name=MembraneChannelsBundle --outfile=dist/bundlefile.js --sourcemap --watch --servedir=.. --serve=80"
  },
  "phet": {
    "requirejsNamespace": "MEMBRANE_CHANNELS",
    "simulation": true,
    "runnable": true,
    "supportedBrands": [
      "phet",
      "phet-io",
      "adapted-from-phet"
    ],
    "supportsOutputJS": true,
    "simFeatures": {
      "supportsDynamicLocale": true,
      "supportsInteractiveDescription": true,
      "supportsSound": true
    }
  }
}