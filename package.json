{
  "name": "membrane-transport",
  "version": "1.0.0-dev.5",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/membrane-transport.git"
  },
  "devDependencies": {
    "grunt": "~1.5.3",
    "esbuild": "^0.24.2",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.1.2"
  },
  "scripts": {
    "dev": "concurrently \"npm run watch-strings\" \"grunt dev-server\" \"grunt dev-server-runnable --port=8123\"",
    "watch-strings": "chokidar \"membrane-transport-strings_en.json5\" \"./strings/MembraneChannels_en.ftl\" -c \"grunt modulify\"",
    "transpileWithoutGrunt": "../perennial/bin/sage run ../chipper/js/grunt/tasks/transpile.ts --live",
    "serve-8081": "npx esbuild ../chipper/dist/js/membrane-transport/js/membrane-transport-main.js --bundle --format=iife --global-name=MembraneChannelsBundle --outfile=dist/bundle.js --sourcemap --watch --servedir=.. --serve=8081",
    "dev81": "concurrently \"npm run transpileWithoutGrunt\" \"npm run serve-8081\""
  },
  "phet": {
    "requirejsNamespace": "MEMBRANE_TRANSPORT",
    "simulation": true,
    "runnable": true,
    "supportedBrands": [
      "phet",
      "phet-io",
      "adapted-from-phet"
    ],
    "supportsOutputJS": true,
    "simFeatures": {
      "supportsDynamicLocale": true,
      "supportsInteractiveDescription": true,
      "supportsSound": true
    },
    "screenNameKeys": [
      "MEMBRANE_TRANSPORT/screen.simpleDiffusion",
      "MEMBRANE_TRANSPORT/screen.facilitatedDiffusion",
      "MEMBRANE_TRANSPORT/screen.activeTransport",
      "MEMBRANE_TRANSPORT/screen.playground"
    ],
    "preload": [
      "../sherpa/lib/d3-interpolate-path-2.3.0.js"
    ]
  }
}